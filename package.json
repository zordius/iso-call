{
  "name": "iso-call",
  "version": "0.0.2",
  "description": "Isomorphic api call for any nodejs/express application",
  "author": "Zordius <zordius@yahoo-inc.com>",
  "contributors": [
    {
      "name": "Zordius Chen",
      "email": "zordius@gmail.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/zordius/iso-call.git"
  },
  "keywords": [
    "isomorphic",
    "client",
    "server",
    "request",
    "http",
    "express",
    "middleware",
    "RPC"
  ],
  "scripts": {
    "test": "mocha",
    "coverage": "istanbul cover -- _mocha",
    "lint": "gulp jscs;eslint ."
  },
  "main": "index.js",
  "dependencies": {
    "request": "2.55.0"
  },
  "devDependencies": {
    "babel-eslint": "3.0.1",
    "eslint": "0.20.0",
    "nock": "1.6.1",
    "browser-request": "0.3.3",
    "body-parser": "1.12.3",
    "babel": "5.1.11",
    "bluebird": "2.9.24",
    "gulp": "3.8.11",
    "gulp-jscs": "1.6.0",
    "chai": "2.2.0",
    "istanbul": "0.3.13",
    "mocha": "2.2.4",
    "sinon": "1.14.1"
  },
  "browserify": {
    "transform": ["aliasify"]
  },
  "aliasify": {
    "aliases": {
      "./iso-request-server": "./lib/iso-request-client",
      "./iso-execute-server": "./lib/iso-execute-client",
      "request": "browser-request",
      "iso-call/polyfill": "babelify/polyfill"
    },
    "global": true
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "bugs": {
    "url": "https://github.com/zordius/iso-call/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/zordius/iso-call/blob/master/LICENSE.txt"
    }
  ],
  "eslintConfig": {
    "env": {
      "node": true,
      "mocha": true
    },
    "parser": "babel-eslint",
    "rules": {
      "yoda": "always",
      "strict": 0,
      "dot-notation": [{"allowKeywords": false}],
      "quotes": ["single"]
    }
  }
}
