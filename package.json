{
  "name": "iso-call",
  "version": "0.0.5",
  "description": "Isomorphic api call for any nodejs/express application",
  "author": "Zordius <zordius@yahoo-inc.com>",
  "contributors": [
    {
      "name": "Zordius Chen",
      "email": "zordius@gmail.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/zordius/iso-call.git"
  },
  "keywords": [
    "isomorphic",
    "client",
    "server",
    "request",
    "http",
    "express",
    "middleware",
    "RPC"
  ],
  "scripts": {
    "test": "mocha",
    "coverage": "istanbul cover -- _mocha",
    "lint": "gulp jscs;eslint ."
  },
  "main": "index.js",
  "dependencies": {
    "body-parser": "^1.18.2",
    "request": "2.83.0"
  },
  "devDependencies": {
    "babel-eslint": "8.0.3",
    "eslint": "4.12.1",
    "nock": "9.1.4",
    "browser-request": "0.3.3",
    "babel-core": "6.26.0",
    "bluebird": "3.5.1",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-jscs": "4.0.0",
    "chai": "4.1.2",
    "istanbul": "0.4.5",
    "mocha": "4.0.1",
    "sinon": "4.1.2"
  },
  "browser": {
    "./lib/iso-request-server.js": "./lib/iso-request-client.js",
    "./lib/iso-execute-server.js": "./lib/iso-execute-client.js"
  },
  "browserify": {
    "transform": [
      "aliasify"
    ]
  },
  "aliasify": {
    "aliases": {
      "request": "browser-request",
      "iso-call/polyfill": "babelify/polyfill"
    },
    "global": true
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "bugs": {
    "url": "https://github.com/zordius/iso-call/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/zordius/iso-call/blob/master/LICENSE.txt"
    }
  ]
}
